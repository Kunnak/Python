def rechnung(x, y, operator):
    if operator == '+':
        summe = int(x) + int(y)
    if operator == '-':
        summe = int(x) - int(y)
    if operator == '/':
        summe = int(x) / int(y)
    if operator == '*':
        summe = int(x) * int(y)
    print(summe)

x = input('X = ')
y = input('Y = ')
operator = input('+ | - | / | *  = ')

rechnung(x, y, operator)
