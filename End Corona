def endcorona(genesene, erkrankte, gesamt):
    tage = 0
    while gesamt > 0:
        gesamt = gesamt - (genesene - erkrankte)
        tage = tage + 1
    print(tage)

genesene = int(input("Genesene: "))
erkrankte = int(input("Erkrankte: "))
gesamt =int(input("Gesammterkrankte: "))

endcorona(genesene, erkrankte, gesamt)
